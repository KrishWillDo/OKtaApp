@using System.Security.Claims
@model ClaimsPrincipal

@{
    ViewData["Title"] = "Profile";
}

<h1>User Profile</h1>
<table>
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>

<a href="/Home/Index">Home</a>
<a href="/account/logout">Logout</a>
